<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <fragment
        android:id="@+id/completedEventsFragment"
        android:name="com.dicoding.submissionevent.ui.CompletedEventsFragment"
        >
        <action
            android:id="@+id/action_completedEvents_to_eventDetail"
            app:destination="@id/eventDetailFragment">
            <argument
                android:name="event"
                app:argType="com.dicoding.submissionevent.data.response.ListEventsItem" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/eventDetailFragment"
        android:name="com.dicoding.submissionevent.ui.EventDetailFragment"
        />

</navigation>
